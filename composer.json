{
    "name": "ackly/l5-php-health",
    "description": "Health check for applications based on Laravel 5",
    "type": "library",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.*",
        "ackly/x-php-health": ">=0.1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Stroganov",
            "email": "a.stroganov@vsemayki.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ackly\\L5Health\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ackly\\L5Health\\HealthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    }
}
